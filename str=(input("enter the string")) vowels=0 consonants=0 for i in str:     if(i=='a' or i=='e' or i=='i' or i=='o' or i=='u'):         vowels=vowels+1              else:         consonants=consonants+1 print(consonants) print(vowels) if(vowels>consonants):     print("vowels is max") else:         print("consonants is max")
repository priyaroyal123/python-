str=(input("enter the string"))
vowels=0
consonants=0
for i in str:
    if(i=='a' or i=='e' or i=='i' or i=='o' or i=='u'):
        vowels=vowels+1
        
    else:
        consonants=consonants+1
print(consonants)
print(vowels)
if(vowels>consonants):
    print("vowels is max")
else:
        print("consonants is max")
