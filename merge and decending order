list1=[]
list2=[]
n=int(input("enter number of elements"))
for i in range(0,n):
    num=int(input("enter number:"))
    list1.append(num)
print(list1)
n=int(input("enter the number of elementss you want"))
for i in range(0,n):
    num=int(input("enter numbers:"))
    list2.append(num)
print(list2)
list3=list1+list2
print(list3)
liste3.sort()
